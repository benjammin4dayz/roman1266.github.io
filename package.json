    "build": "npm run build-js && npm run build-css"
  },
  "dependencies": {
    "csso": "^5.0.5",
    "csso-cli": "^4.0.2",
    "terser": "^5.19.2"
  },
  "devDependencies": {
    "eslint": "^8.7.0",
    "live-server": "^1.2.2",
    "prettier": "^2.5.1"
  }
}
